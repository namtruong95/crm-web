{
  "name": "mt-angular-v6",
  "version": "0.0.1",
  "author": "Nam Truong",
  "scripts": {
    "prettier:check": "./node_modules/.bin/prettier --config ./.prettierrc --ignore-path ./.prettierignore --list-different \"src/{app,assets,constants,interfaces,models,environments,shared,styles}/**/*{.ts,.js,.json,.scss,.md}\"",
    "prettier:fix": "./node_modules/.bin/prettier --config ./.prettierrc --ignore-path ./.prettierignore --write \"src/{app,assets,constants,interfaces,models,environments,shared,styles}/**/*{.ts,.js,.json,.scss,.md}\"",
    "lint:js": "./node_modules/.bin/ng lint",
    "serve:local": "./node_modules/.bin/ng serve --configuration=local --port=3000",
    "serve:dev": "./node_modules/.bin/ng serve --configuration=dev --port=3000",
    "build:stg": "rm -rf crm_web.zip && ./node_modules/.bin/ng build --configuration=stg --progress=false && yarn compress",
    "build:dev": "rm -rf crm_web.zip && ./node_modules/.bin/ng build --configuration=dev --progress=false && yarn compress",
    "build:prod": "rm -rf crm_web.zip && ./node_modules/.bin/ng build --configuration=prod --progress=false && yarn compress",
    "compress": "./node_modules/.bin/gulp compress",
    "compressSrc": "rm -rf src.zip && ./node_modules/.bin/gulp compressSrc",
    "deliver": "./node_modules/.bin/ng build --configuration=prod --progress=false && ./node_modules/.bin/gulp compress"
  },
  "private": true,
  "dependencies": {
    "@agm/core": "^1.0.0-beta.3",
    "@agm/snazzy-info-window": "^1.0.0-beta.3",
